
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading">Galeria</h2>
      </div>
    </div>
    <div id="image-galery">
      <% photos.forEach(function(collection){%>
        <div class="row">
          <% collection.forEach(function(photo){ %>
            <div class="col-md-4" align="center" >
              <img src="<%= photo.split("public")[1]%>" class="img-thumbnail galery" alt="Cinque Terre" width="304" height="236">
            </div>
          <%});%>
        </div>
      <%});%>
    </div>

    <div id="myModal" class="modal-galery">
      <span class="close">&times;</span>
        <img class="modal-galery-content" id="modalImage">
      <div id="caption"></div>
    </div>
  </div>
